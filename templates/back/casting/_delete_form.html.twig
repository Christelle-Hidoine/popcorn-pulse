<form method="post" action="{{ path('app_back_casting_delete', {'id': casting.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ casting.id) }}">
    <button class="btn btn-outline-danger btn-md review-btn mb-2">Delete</button>
</form>